% Am ales ca lungimea vectorului 'a' sa fie de 10 elemente 
a=rand(1,10)+j*rand(1,10)
% Astfel a rezultat un vector 1x10 format din elemente complexe


function [medie , patrat ,inmultire]= fct(c)  % sintaxa functiei
lungime = length (a)
medie = real(sum(a)) / lungime                  % media
patrat = a.^2                                   % vectorul ridicat la patrat
inmultire = a * a.'                             % matrice formata prin inmultirea vectorului cu transpusul
